<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.processmanager.PrimaryController">
    <TableView fx:id="processTableView" prefHeight="400" prefWidth="600">
        <columns>
            <TableColumn fx:id="processIDColumn" prefWidth="150" text="Process ID" />
            <TableColumn fx:id="nameColumn" prefWidth="150" text="Process Name" />
            <TableColumn fx:id="userColumn" prefWidth="150" text="User" />
            <TableColumn fx:id="commandLineColumn" prefWidth="150" text="Command Line" />
        </columns>
    </TableView>
</VBox>
